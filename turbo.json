{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "dev": {
      "dotEnv": [
        ".env.development.local",
        ".env.local",
        ".env.development",
        ".env"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dis/**"],
      "cache": false
    },
    "test": {
      "cache": false
    },
    "lint": {}
  }
}
